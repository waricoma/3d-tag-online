{
  "rules": {
    "ja-space-after-question": "true",
    "max-ten": "true",
    "no-dead-link": "true",
    "no-mix-dearu-desumasu": "true",
    "preset-ja-spacing": "true",
    "preset-ja-technical-writing": "true",
    "preset-japanese": "true",
    "preset-jtf-style": "true",
    "prh": "true",
    "spellcheck-tech-word": "true",
  },
  "filters": {
    "comments": true
  },
  "plugins": {
    "@textlint/markdown": true
  }
}
